}

.levelcontainer {
  position: relative;
  margin-left: 60rpx;
}

.levelcontainer .navline0 {
  position: absolute;
  height: 100%;
  width: 1rpx;
  left: 18rpx;
  top: -8rpx;
  z-index: 2;
  border-left: 1rpx dashed #1982c2;
}

.levelcontainer .navline1 {
  position: absolute;
  height: 100%;
  width: 1rpx;
  top: -28rpx;
  left: 18rpx;
  z-index: 2;
  border-left: 1rpx dashed #1982c2;
}

.levelcontainer .level {
  position: relative;
  display: flex;
  direction: row;
  justify-content: start; 
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.levelcontainer .level .levelh {
  border-bottom: 1rpx dashed #1982c2;
  width: 100rpx;
  height: 1rpx;
  margin-left: 18rpx;
  margin-top: 18rpx;
}

.levelcontainer .level .levelsubtext {
  font-size: 28rpx;
  color:#a7a3a3;
}

