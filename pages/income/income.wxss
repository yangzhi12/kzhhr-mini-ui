page{
    height: 100%;
    width: 100%;
    background: #f4f4f4;
}
.container{
    background: #f4f4f4;
    height: auto;
    overflow: hidden;
    width: 100%;
}

.profile-info {
  background: #1982c2;
  padding-bottom: 10rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
  color: #fff;
}

.user-menu{
    width: 100%;
    height: auto;
    overflow: hidden;
    background: rgb(146, 68, 68);
    padding-left: 20rpx;
    padding-right: 20px;
}

.reporttainer{
  margin-top: 20rpx;
  background: #fff;
  padding: 20rpx 20rpx;
}

.reporttitle{
  text-align: center;
  padding-bottom: 10rpx;
  border-bottom: 1rpx solid #1982c2;
  margin-left: 200rpx;
  margin-right: 200rpx;
}

.reportcontent{
  margin-top: 20rpx;
}

.tablerow {
  display: flex;
  direction: column;
  justify-content: space-between;
}

.tableth {
  text-align: center;
  border-bottom: 1rpx solid #f4f4f4;
  width: 20%;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.tabletd {
  text-align: center;
  border-bottom: 1rpx solid #f4f4f4;
  width: 20%;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.tdlable {
  text-align: center;
  border-bottom: 1rpx solid #f4f4f4;
  width: 20%;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.tablefooter{
  display: flex;
  direction: column;
  justify-content: flex-end;
}

.footervalue {
  text-align: center;
  padding-top: 5rpx;
  padding-right: 20rpx;
  padding-bottom: 10rpx;
  color: red;
}